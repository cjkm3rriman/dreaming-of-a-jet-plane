# Live Flight Providers
LIVE_AIRCRAFT_PROVIDER=fr24  # Primary provider key (fr24 or airlabs)
LIVE_AIRCRAFT_PROVIDER_FALLBACKS=airlabs  # Comma-separated fallback providers
PROVIDER_OVERRIDE_SECRET=your_provider_override_secret
FR24_API_KEY=your_flightradar24_api_key_here
AIRLABS_API_KEY=your_airlabs_api_key_here

# TTS Provider Configuration
TTS_PROVIDER=elevenlabs  # Options: elevenlabs, google, inworld, fallback
ELEVENLABS_TEXT_TO_VOICE_API_KEY=your_elevenlabs_api_key_here

# Provider Override (for testing both aircraft and TTS providers via query parameters)
# Generate a secure secret with: python -c "import secrets; print(secrets.token_urlsafe(32))"
# Uses PROVIDER_OVERRIDE_SECRET defined above

# Optional Analytics
MIXPANEL_TOKEN=your_mixpanel_project_token_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# S3 Caching (optional)
S3_BUCKET_NAME=your_s3_bucket_name
AWS_S3_REGION=us-east-2  # Region for S3 bucket

# Google Gemini TTS Configuration (if using google provider)
GOOGLE_API_KEY=your_google_api_key_here
