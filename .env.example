# Required API Keys
FR24_API_KEY=your_flightradar24_api_key_here
ELEVENLABS_TEXT_TO_VOICE_API_KEY=your_elevenlabs_api_key_here

# TTS Provider Configuration
TTS_PROVIDER=elevenlabs  # Options: elevenlabs, polly, google, fallback

# TTS Provider Override (for testing different providers via query parameters)
# Generate a secure secret with: python -c "import secrets; print(secrets.token_urlsafe(32))"
TTS_PROVIDER_OVERRIDE_SECRET=your_secret_key_here

# Optional Analytics
MIXPANEL_TOKEN=your_mixpanel_project_token_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# S3 Caching (optional)
S3_BUCKET_NAME=your_s3_bucket_name
AWS_S3_REGION=us-east-2  # Region for S3 bucket

# AWS Polly TTS (if using polly provider)
AWS_POLLY_REGION=us-east-1  # Required region for Polly neural voices

# Google Gemini TTS Configuration (if using google provider)
GOOGLE_API_KEY=your_google_api_key_here